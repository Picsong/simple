{
  "name": "simple",
  "version": "0.1.0",
  "description": "使用 TypeScript，未使用任何 UI 库。",
  "dependencies": {
    "@alifd/next": "^1.20.28",
    "@alifd/theme-design-pro": "^0.6.2",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "ahooks": "^2.6.1",
    "moment": "^2.29.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "@ice/spec": "^1.0.1",
    "build-plugin-fusion": "^0.1.7",
    "build-plugin-ice-auth": "^1.7.1",
    "eslint": "^6.0.1",
    "ice.js": "^1.0.0",
    "stylelint": "^13.0.0"
  },
  "scripts": {
    "start": "icejs start --mode local",
    "build": "icejs build --mode local",
    "build:daily": "icejs build --mode daily",
